{
  "name": "@wolfzwz/javascript-tool-library",
  "version": "1.0.8",
  "description": "a javascript tools package",
  "main": "dist/javascript-tool-library.js",
  "files": [
    "dist",
    "assets"
  ],
  "engines": {
    "node": ">=12.14.1"
  },
  "scripts": {
    "test": "jest ./test",
    "docs": "jsdoc --configure jsdoc.config.json",
    "lint": "eslint --ext .ts ./",
    "eslint-format": "eslint --fix --ext .ts ./",
    "commit": "git add . && npx git-cz",
    "build": "rollup -c",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolfzwz/javascript-tool-library.git"
  },
  "keywords": [
    "tool",
    "javascipt",
    "utils"
  ],
  "author": "wolfzwz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolfzwz/javascript-tool-library/issues"
  },
  "homepage": "https://github.com/wolfzwz/javascript-tool-library#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.6",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  }
}
