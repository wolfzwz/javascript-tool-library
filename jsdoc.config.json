{
  "source": {
    "include": ["package.json", "./src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "docdash": {
    "search": [true]
  }
}
